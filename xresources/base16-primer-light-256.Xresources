! Base16 Primer Light
! Scheme: Jimmy Lin

#define base00 #fafbfc
#define base01 #e1e4e8
#define base02 #d1d5da
#define base03 #959da5
#define base04 #444d56
#define base05 #2f363d
#define base06 #24292e
#define base07 #1b1f23
#define base08 #d73a49
#define base09 #f66a0a
